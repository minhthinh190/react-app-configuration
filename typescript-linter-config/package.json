    "@typescript-eslint/parser": "^6.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
